{
  "name": "spatialreference",
  "version": "1.1.2",
  "description": "A helper library for translating spatial reference systems",
  "main": "dist/index.js",
  "scripts": {
    "test": "standard && tape test/*.js | tap-spec",
    "clean": "rm -rf dist",
    "compile": "buble -i src -o dist"
  },
  "repository": {
